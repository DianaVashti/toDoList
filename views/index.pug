extends layout

block content
  form(action=`/home`)
    .form-group
      label(for='inputEmail') Email address
      input#inputEmail.form-control(type='email', aria-describedby='emailHelp', placeholder='person@me.com')
      small#emailHelp.form-text.text-muted We'll never share your email with anyone else.
    .form-group
      label(for='inputPassword') Password
      input#inputPassword.form-control(type='password', placeholder='poop')
    button.btn.btn-primary(type='submit') Submit
